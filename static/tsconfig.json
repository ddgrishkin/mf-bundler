{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "jsx": "react",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "typedefs/**/*",
        "src/**/*"
    ]
}
